{
  "manifest_version": 2,

  "name": "Health Rating",
  "description": "Awesomeness",
  "version": "1.0",

  "permissions": [
    "https://*/*",
    "http://*/*"
  ],

  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": [
        "lib/jquery-2.1.3.min.js",
        "lib/lodash-2.4.1.min.js",
        "lib/coffee-script.js",
        "src/ingredients.js",
        "src/boot-extension.js"
      ],
      "css" : [
        "hackathon.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": ["*://www.google.com/*"],
      "js": [
        "lib/jquery-2.1.3.min.js",
        "lib/lodash-2.4.1.min.js",
        "lib/coffee-script.js",
        "src/ingredients.js",
        "src/boot-google.js"
      ]
    },
    {
      "matches": ["*://www.bing.com/*"],
      "js": [
        "lib/jquery-2.1.3.min.js",
        "lib/lodash-2.4.1.min.js",
        "lib/coffee-script.js",
        "src/ingredients.js",
        "src/boot-bing.js"
      ]
    }
  ],
  "web_accessible_resources":[
    "*",
    "dist/*",
    "src/*"
  ]
}